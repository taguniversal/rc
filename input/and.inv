// Define inputs
object pin_A(1) @ (-2, 0, 0);
object pin_B(1) @ (-2, -2, 0);

// Define AND gate
object and_gate(2) @ (0, -1, 0);
material "metallic" apply and_gate;

// Define output
object pin_out(1) @ (2, -1, 0);

// Link logic
link pin_A -> and_gate;
link pin_B -> and_gate;
link and_gate -> pin_out;

// Export to USD
export usd "and_gate.usda";
