<Definition>
  <meta>
    <xpath>
      <findInputs>/IO/inputs/input</findInputs>
      <findOutputs>/IO/outputs/output</findOutputs>
      <findInputByName>concat('/IO/inputs/input[@name=\"', $inputName, '\"]')</findInputByName>
      <findOutputByName>concat('/IO/outputs/output[@name=\"', $outputName, '\"]')</findOutputByName>
    </xpath>
    <version>1.0</version>
    <description>Standard single-input NOT gate definition with conditional truth table.</description>
  </meta>

  <name>NOT</name>

 <SourceList>
    <SourcePlace name="A"/>
  </SourceList>

  <DestinationList>
    <DestinationPlace name="Out0"/>
  </DestinationList>


  <ConditionalInvocation>
    <invocationName>$A</invocationName>
    <case value="0">1</case>
    <case value="1">0</case>
  </ConditionalInvocation>
</Definition>
