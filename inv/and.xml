<Definition>
  <Name>AND</Name>

  <!-- Internally read from these -->
  <SourceList>
    <SourcePlace name="A"/>
    <SourcePlace name="B"/>
  </SourceList>

  <!-- Internally write to these -->
  <DestinationList>
    <destination name="$out"/>
  </DestinationList>

  <ConditionalInvocation>
   <InvocationName>$A$B</InvocationName>
    <Case value="00">0</Case>
    <Case value="01">0</Case>
    <Case value="10">0</Case>
    <Case value="11">1</Case>
  </ConditionalInvocation>
</Definition>
